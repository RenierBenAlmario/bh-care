@page
@model Barangay.Pages.Nurse.ReportsModel
@{
    ViewData["Title"] = "Reports";
    Layout = "/Pages/Shared/_NurseLayout.cshtml";
}

<div class="container mt-4">
    <h2 class="mb-4">Reports</h2>
    <div class="card">
        <div class="card-header bg-primary text-white">
            <div class="d-flex justify-content-between align-items-center">
                <h5 class="mb-0">Health Reports</h5>
                <div>
                    <button class="btn btn-sm btn-light">Generate Report</button>
                </div>
            </div>
        </div>
        <div class="card-body">
            <p class="text-muted">This page will display various health reports and statistics.</p>
        </div>
    </div>
</div> 