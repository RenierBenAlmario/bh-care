                // For demo purposes, we'll use a simple base64 encoding with a prefix
                // In production, this would use proper RSA encryption with a public key
                var bytes = btoa(textToEncrypt);
                return `ASYMMETRIC_ENCRYPTED_${bytes}`;
            } catch (e) {
